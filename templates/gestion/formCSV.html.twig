{% extends 'base.html.twig' %}

{% block body %}
    <h1 class="text-site-color">Import Users</h1>

    {{ form_start(form) }}<br>
    {{ form_row(form.csvFile) }}<br>
    <button class="btn btn-secondary-custom">Import</button>
    {{ form_end(form) }}

    {% for message in app.flashes('success') %}
        <div class="alert alert-success">{{ message }}</div>
    {% endfor %}
    {% for message in app.flashes('error') %}
        <div class="alert alert-danger">{{ message }}</div>
    {% endfor %}
{% endblock %}
